      function createFlippedImage(src) {
        var img = createImageElement(src, "124px", "112px");
        img.classList.add("room-image");
        img.style.transform = "scaleX(-1)";
  
        // Add flipped attribute
        img.setAttribute('data-flipped', 'true');
        
        return img;
      }

      var totalPrice = prices[selectedImage1] + prices[selectedImage2] + prices[selectedImage3] + prices[selectedImage4] + prices[selectedImage5] + prices[selectedImage6] + prices[selectedImage7];
      var totalPriceElement = document.getElementById("totalPrice");
      totalPriceElement.textContent = "Total Price: $" + totalPrice.toFixed(2);
    
      var linkElement = document.createElement("a");
      linkElement.href = prices[selectedImage1].link; 
      linkElement.textContent = prices[selectedImage1].link;
      document.body.appendChild(linkElement);
    }

    function getRandomElement(array) {
      var randomIndex = Math.floor(Math.random() * array.length);
      return array[randomIndex];
    }

    function handleImageClick(event) {
      var img = event.target;
  
      if (img.tagName === 'IMG') {
        var imgSrc = img.src;
        var url = siteMap[imgSrc];
        
        if (url) {
          window.open(url);
        }
      }
    }
  
    imageContainer.addEventListener('dblclick', handleImageClick);


    function createImageElement(src, width, height) {
      var img = document.createElement("img");
      img.src = src;
      img.classList.add("room-image");
      img.style.width = width;
      img.style.height = height;
      return img;
    }

    function addImage(src) {
      var img = createImageElement(src);
      
      // set initial size
      img.style.width = '200px';
      img.style.height = '150px';

      // set initial position
      img.style.transform = 'translate(100px, 100px)';

      // set data attributes
      img.setAttribute('data-x', 100);
      img.setAttribute('data-y', 100);

      imageContainer.appendChild(img);
    }


    // New function to duplicate and flip an image
function duplicateAndFlipImage(src) {
  var img = createImageElement(src, "124px", "112px");
  img.classList.add("room-image");
  
  var flippedImg = createImageElement(src, "124px", "112px");
  flippedImg.style.transform = "scaleX(-1)";
  
  return [img, flippedImg];
}


    // Helper functions
    function flipImages(imagePaths) {
      return imagePaths.map(path => {
        return path.replace('.png', '-flipped.png'); 
      });
    }

    function createFlippedImage(src) {
      var img = createImageElement(src, "124px", "112px");
      img.classList.add("room-image");
      img.style.transform = "scaleX(-1)";
      return img;
    }

